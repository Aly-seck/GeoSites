{
	"order" : 1,
	"id": "default", 
	"clustering": false,
	"name": "Parc national des Ecrins", 
	"center": {
		"lat": 44.85,
		"lng": 6.1,
		"zoom": 10
	},
	"location":{
		"name":"Communes",
		"url":"data/communes.json"
	},
	"layers": { 
		"baselayers": [
		    {
		        "name": "OpenCycleMap",
		        "type": "xyz",
		        "url": "http://{s}.tile.opencyclemap.org/cycle/{z}/{x}/{y}.png",
		        "active": false,
		        "options":{}
		    },
		    {
		        "name": "OpenStreetMap",
		        "type": "xyz",
		        "url": "http://{s}.tile.osm.org/{z}/{x}/{y}.png",
		        "active": true,
		        "options":{}
		    }
		],
		"overlay":{
		        "name": "Perimetre quietude",
		        "type": "geojson",
		        "url": "data/site_geol.geojson",
		        "options" : "{style:function (feature) {return {color: $scope.mapOptions.layers.overlay.filters.ETAGE_ALT.values[feature.properties.ETAGE_ALT].color, weight: 0.1, opacity: 1, fillOpacity: 0.7 };},onEachFeature: function (feature, layer) {if (feature.properties, {noHide:false}) {layer.bindPopup(\"<h4>\"+feature.properties.NOM1+\"</h4>\");}}}",
		        "filters":{
					"ETAGE_ALT" :{
						"name" : "Etage altitude",
						"values" : {
							"HM":{"label":"Haute montagne", "color":"#FF6A00", "visible":true, "periode":{"1":0, "2":0, "3":1, "4":1, "5":1, "6":1, "7":1, "8":1, "9":0, "10":0, "11":0, "12":0}},
							"MM":{"label":"Moyenne montagne", "color":"maroon", "visible":true, "periode":{"1":0, "2":0, "3":1, "4":1, "5":1, "6":1, "7":1, "8":1, "9":0, "10":0, "11":0, "12":0}}
						}
					},
					"PROP_TYPE1" : {
						"name":"Type propriétés",
						"values" : {
							"PRI":{"label":"Privé","visible":true},
							"DOM":{"label":"Domanial","visible":true},
							"COM":{"label":"Communal","visible":true}
						}
					}
		        }
		    }
	}
}
